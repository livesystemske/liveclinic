{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "Skoruba": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "restrictedToMinimumLevel": "Error",
                    "path": "Log/liveclinic_billing_.log",
                    "rollingInterval": "Day"
                }
            }
        ]
    }
}